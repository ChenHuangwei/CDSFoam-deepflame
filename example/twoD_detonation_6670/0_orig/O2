FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.101;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    left
    {
        type            zeroGradient;
    }
    wand
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
}


